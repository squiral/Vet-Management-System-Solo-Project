<h2>Animals</h2>


<% @animals.each do |animal| %>

  <div class="animal">
    <p>Name: <%= animal.name() %></p>
    <p>DoB: <%= animal.date_of_birth() %></p>
    <p>Type: <%= animal.type() %></p>
    <p>Vet: <%= animal.vet.name() %></p>
    <!-- <p><a href="/animals/<%= animal.id %>"><%=animal.name()%>'s Profile</a></p> -->
  </div>

<% end %>
